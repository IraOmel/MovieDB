{
    "builds": [{
        "src": "movie_database/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.2 " }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "movie_database/wsgi.py"
        }
    ]
}